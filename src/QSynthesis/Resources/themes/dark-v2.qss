/* Global */

QWidget {
    font-size: 15px;
    outline: none;
}

CTopWidget {
    qproperty-resizeMargins: 6px;
}


/* Window */

CWindowBarV2 {
    background-color: #3C3C3C;
    qproperty-titleColor: #ECECEC;
    qproperty-titleMargin: 20px;
}

CWindowBarV2>QLabel#win-title-label {
    padding: 0;
    border: none;
    color: transparent;
    background-color: transparent;
    /* Limit window bar height */
    min-height: 40px;
}

CWindowBarV2>CToolButton#min-button {
    qproperty-iconUp: svg(":/svg/window/minimize.svg", #FFFFFF);
    qproperty-iconSize: 12px 12px;
    min-width: 50px;
    border: none;
    padding: 0;
    background-color: transparent;
}

CWindowBarV2>CToolButton#min-button:hover,
CWindowBarV2>CToolButton#min-button:pressed {
    background-color: rgba(0, 0, 0, 15%);
}

CWindowBarV2>CToolButton#max-button {
    qproperty-iconUp: svg(":/svg/window/maximize.svg", #FFFFFF);
    qproperty-iconUpChecked: svg(":/svg/window/maximize2.svg", #FFFFFF);
    qproperty-iconSize: 12px 12px;
    min-width: 50px;
    border: none;
    padding: 0;
    background-color: transparent;
}

CWindowBarV2>CToolButton#max-button:hover,
CWindowBarV2>CToolButton#max-button:pressed {
    background-color: rgba(0, 0, 0, 15%);
}

CWindowBarV2>CToolButton#win-close-button {
    qproperty-iconUp: svg(":/svg/window/close.svg", #FFFFFF);
    qproperty-iconSize: 12px 12px;
    min-width: 50px;
    border: none;
    padding: 0;
    background-color: transparent;
}

CWindowBarV2>CToolButton#win-close-button:hover,
CWindowBarV2>CToolButton#win-close-button:pressed {
    background-color: #e81123;
}

CWindowBarV2>CToolButton#icon-button {
    qproperty-iconUp: svg(":/svg/app/logo-white.svg");
    qproperty-iconSize: 20px 20px;
    min-width: 40px;
    border: none;
    padding: 0;
    background-color: transparent;
}


/* Dialog */

CDialogV2 {
    background-color: #333333;
}

CDialogBarV2>CToolButton#win-close-button {
    qproperty-iconUp: svg(":/svg/dialog/close.svg", #FFFFFF);
    qproperty-iconSize: 12px 12px;
    min-width: 50px;
    min-height: 40px;
    border: none;
    padding: 0;
    background-color: transparent;
}

CDialogBarV2>CToolButton#win-close-button:hover {
    background-color: rgba(255, 255, 255, 15%);
}

CDialogBarV2>CToolButton#win-close-button:pressed {
    background-color: rgba(255, 255, 255, 30%);
}


/* Menu Bar */

CWindowBarV2>CMenuBar {
    background-color: transparent;
    border: none;
    qproperty-extensionIcon: svg(":/svg/menu-bar/more-line.svg", white);
}

CWindowBarV2>CMenuBar>QToolButton#qt_menubar_ext_button:hover,
CWindowBarV2>CMenuBar>QToolButton#qt_menubar_ext_button:pressed {
    background-color: rgba(255, 255, 255, 10%);
}

CWindowBarV2>CMenuBar::item {
    color: #CCCCCC;
    border: none;
    padding: 10px 12px;
}

CWindowBarV2>CMenuBar::item:selected {
    background-color: rgba(255, 255, 255, 10%);
}


/* Old Menu */

CMenu {
    padding: 5px 5px;
    background: #252526;
    border: 1px solid transparent;
}

CMenu::item {
    padding: 6px 30px;
    background: transparent;
    color: #CCCCCC;
}

CMenu::item:selected {
    color: white;
    background-color: #0060C0;
}

CMenu::item:disabled {
    color: #666666;
}

CMenu::separator {
    height: 1px;
    background-color: #5B5B5B;
    margin: 6px 6px 6px 6px;
}

CMenu::indicator {
    width: 20px;
    height: 20px;
    padding-left: 5px;
    padding-right: 0;
}

CMenu::indicator:unchecked {
    image: none;
}

CMenu::indicator:checked {
    image: url(:/svg/menu/dark/checked-line.svg);
}

CMenu::right-arrow {
    image: url(:/svg/menu/dark/arrow-right-s-line.svg);
    width: 20px;
    height: 20px;
    padding: 0 5px;
}


/* Central Tab Widget */

CentralTabCloseButton {
    qproperty-iconUp: svg(":/svg/tab-button/close-line.svg", #D4D4D4);
    qproperty-iconUpChecked: svg(":/svg/tab-button/close-line.svg", #808080);
    qproperty-iconSize: 11px 11px;
    border-radius: 3px;
    width: 18px;
    height: 18px;
    padding: 0;
    border: none;
    margin-right: 1px;
    background-color: transparent;
}

CentralTabCloseButton:hover {
    background-color: rgba(255, 255, 255, 10%);
}

CentralTabCloseButton:pressed {
    background-color: rgba(255, 255, 255, 7.5%);
}

QToolButton#tab-icon-button {
    border: none;
    background-color: transparent;
    fix-width: 18px;
    fix-height: 18px;
    padding: 3px;
    margin-left: 3px;
}

QToolButton#tab-icon-button[tab-type=welcome] {
    image: url(:/svg/tab-icon/welcome-light.svg);
}

QToolButton#tab-icon-button[tab-type=tuning] {
    image: url(:/svg/tab-icon/dark/itunes-note-brands.svg);
}

QToolButton#tab-icon-button[tab-type=voice] {
    image: url(:/svg/tab-icon/dark/compact-disc-solid.svg);
}

QToolButton#tab-icon-button[tab-type=keyboard],
QToolButton#tab-icon-button[tab-type=setting] {
    image: url(:/svg/tab-icon/dark/sliders-h-solid.svg);
}

CentralTabWidget {
    background-color: #252526;
}

CentralTab,
CentralTabContent {
    background-color: #1E1E1E;
}

CentralTabWidget::pane {
    border: 0;
}

CentralTabWidget::tab-bar {
    subcontrol-position: left;
}

CentralTabWidget>QTabBar {
    background-color: transparent;
}

CentralTabWidget>QTabBar::scroller {
    width: 80px;
}

CentralTabWidget>QTabBar::tear {
    width: 0px;
    border: none;
}

CentralTabWidget>QTabBar::tab {
    color: #A0A0A0;
    padding: 9px 15px;
    border-left: 1px solid #252526;
    border-right: 1px solid #252526;
    background-color: #2D2D2D;
}

CentralTabWidget>QTabBar::tab:selected {
    color: white;
    background-color: #1E1E1E;
}

CentralTabWidget>QTabBar::tab:!selected:hover {
    background-color: #333333;
}

CentralTabWidget>QTabBar>QToolButton::left-arrow {
    image: url(:/svg/tab-button/dark/arrow-left-line.svg);
}

CentralTabWidget>QTabBar>QToolButton::right-arrow {
    image: url(:/svg/tab-button/dark/arrow-right-line.svg);
}

CentralTabWidget>QTabBar>QToolButton::left-arrow:disabled {
    image: url(:/svg/tab-button/dark/arrow-left-line-disabled.svg);
}

CentralTabWidget>QTabBar>QToolButton::right-arrow:disabled {
    image: url(:/svg/tab-button/dark/arrow-right-line-disabled.svg);
}

CentralTabWidget>QTabBar>.QToolButton {
    border: 1px solid transparent;
    background-color: #222222;
    padding: 8px;
}

CentralTabWidget>QTabBar>.QToolButton:hover {
    background-color: #333333;
}

CentralTabWidget>QTabBar>.QToolButton:pressed {
    background-color: #292929;
}

CentralTabBar {
    qproperty-dragOffset: qmargins(0, 40px, 0, 40px);
}


/* Dialog */

QLabel#dialog-title-label {
    margin-top: 10px;
    font-size: 20px;
    color: white;
}

QLabel#dialog-caption-label {
    color: white;
}

QCheckBox#dialog-check-box {
    color: white;
}

QPushButton#dialog-button {
    color: white;
    background-color: #0E639C;
    border: 1px solid transparent;
    fix-width: 115px;
    fix-height: 35px;
}

QPushButton#dialog-button:hover {
    background-color: #1177BB;
}

QPushButton#dialog-button:pressed {
    color: #EEEEEE;
}


