/* WelcomeMainArea */

WelcomeMainArea {
    qproperty-ustIcon: url(":/svg/image/ust-icon.svg");
    qproperty-dirIcon: url(":/svg/image/folder-icon.svg");
    qproperty-iconSize: 40px 40px;
    qproperty-layoutMargins: qmargins(40px, 20px, 40px, 20px);
    qproperty-layoutSpacing: 0;
    qproperty-topLayoutMargins: qmargins(0, 0, 0, 20px);
    qproperty-topLayoutSpacing: 0;
    qproperty-switchLayoutMargins: 0;
    qproperty-switchLayoutSpacing: 25px;
}

WelcomeMainArea>.SwitchButton {
    color: #666666;
    font-size: 18px;
    padding: 10px 0;
    background-color: transparent;
    border: none;
}

WelcomeMainArea>.SwitchButton:hover {
    color: #DBDBDB;
}

WelcomeMainArea>.SwitchButton:checked {
    color: #DBDBDB;
}

WelcomeMainArea>.SwitchButtonUnderline {
    qproperty-lineWidth: 4px;
    qproperty-color: #0E639C;
}

WelcomeMainArea>FileListWidget {
    border: none;
    background-color: transparent;
    qproperty-styleData: qvariants(/* styleData */
    qtypeface(rgba(255, 255, 255, 5%), 1), /* selectType */
    qtypeface(#3A3938, 1), /* underline */
    qtypeface(#E0E0E0, 17px), /* fileType */
    qtypeface(#909090, 15px), /* locType */
    qtypeface(#909090, 15px), /* dateType */
    qmargins(10px, 10px, 10px, 1px), /* fileMargins */
    qmargins(10px, 1px, 10px, 10px), /* locMargins */
    qmargins(10px, 10px, 20px, 10px), /* dateMargins */
    qmargins(20px, 10px, 10px, 10px), /* iconMargins */
    );
}

WelcomeMainArea>QLabel#no-data-label {
    color: white;
}


