/* VDirTreeWidget */

VDirTreeWidget {
    qproperty-metaIcon: svg(":/svg/voice-dir-panel/database.svg", #519ABA);
    qproperty-otoIcon: svg(":/svg/voice-dir-panel/folder.svg", #FFCC66);
    qproperty-charIcon: svg(":/svg/voice-dir-panel/user.svg", #CCCCCC);
    qproperty-readmeIcon: svg(":/svg/voice-dir-panel/gnome-mime-text-x-readme.svg", #CCCCCC);
    qproperty-prefixIcon: svg(":/svg/voice-dir-panel/accessories-dictionary.svg", #CCCCCC);
    background-color: #242425;
    border: none;
    outline: none;
}

VDirTreeWidget::item {
    border: 1px solid transparent;
    background-color: transparent;
    color: #CCCCCC;
}

VDirTreeWidget::item:hover {
    background-color: rgba(255, 255, 255, 15%);
}

VDirTreeWidget::item:selected {
    border-color: #007FD4;
    background-color: rgba(0, 127, 212, 30%);
    color: white;
}

VDirTreeWidget::branch:open:has-children {
    border-image: none;
    image: url(:/svg/voice-dir-panel/arrow-down-s-line.svg);
}

VDirTreeWidget::branch:closed:has-children {
    border-image: none;
    image: url(:/svg/voice-dir-panel/arrow-right-s-line.svg);
}

VDirTreeDragLabel {
    border: 1px solid #007FD4;
    background-color: rgba(0, 127, 212, 50%);
    color: white;
    padding: 2px;
}