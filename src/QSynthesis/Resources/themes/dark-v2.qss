/* ComboSelector */

ComboSelector {
    background-color: transparent;
    min-width: 600px;
    min-height: 400px;
    qproperty-layoutMargins: 10px;
    qproperty-layoutSpacing: 12px;
    qproperty-wallpaper: #252526;
    qproperty-margins: qmargins(0, 0, 0, 0);
    /* border-top: 10px solid qlineargradient(y0:0, y1:1, stop: 0 #ECECEC, stop: 1 transparent); */
    border-top: none;
    border-left: 8px solid qlineargradient(x0: 0, x1: 1, stop: 0 rgba(0, 0, 0, 25%), stop: 1 transparent);
    border-bottom: 8px solid qlineargradient(y0: 0, y1: 1, stop: 0 transparent, stop: 1 rgba(0, 0, 0, 25%));
    border-right: 8px solid qlineargradient(x0: 0, x1: 1, stop: 0 transparent, stop: 1 rgba(0, 0, 0, 25%));
}

ComboSelector>QListWidget {
    border: none;
    outline: 0;
    background-color: transparent;
}

ComboSelector>QListWidget::item {
    font-size: 15px;
    color: #CCCCCC;
    padding: 3px 3px;
    background-color: transparent;
}

ComboSelector>QListWidget::item:hover {
    background-color: rgba(255, 255, 255, 10%);
}

ComboSelector>QListWidget::item::selected {
    background-color: rgba(0, 96, 192, 25%)
}

ComboSelector>QLineEdit {
    padding: 3px 3px;
}

ComboSelector>QLineEdit {
    border: 1px solid transparent;
    color: #CCCCCC;
    background-color: #3C3C3C;
}

ComboSelector>QLineEdit:focus {
    border-color: #167FD2;
}


/* CScrollBar */

CScrollArea {
    border: none;
    background-color: transparent;
}

CGraphicsView {
    border: none;
    background-color: transparent;
}

CScrollBar {
    background: transparent;
    margin: 0;
}

CScrollBar:vertical {
    width: 10px;
}

CScrollBar:horizontal {
    height: 10px;
}

CScrollBar::handle:vertical {
    min-height: 40px;
}

CScrollBar::handle:horizontal {
    min-width: 40px;
}

CScrollBar::handle {
    background: rgba(255, 255, 255, 0.3);
    border: 0px;
    /* margin: 4px 4px 4px 4px; */
}

CScrollBar::handle:hover {
    background: rgba(255, 255, 255, 0.4);
}


/* CScrollBar::handle:pressed {
    background: rgba(0, 0, 0, 18%);
} */

CScrollBar::sub-page {
    background: transparent;
}

CScrollBar::add-page {
    background: transparent;
}

CScrollBar::up-arrow {
    background: transparent;
}

CScrollBar::down-arrow {
    background: transparent;
}

CScrollBar::left-arrow {
    background: transparent;
}

CScrollBar::right-arrow {
    background: transparent;
}

CScrollBar::sub-line {
    background: transparent;
    width: 0px;
}

CScrollBar::add-line {
    background: transparent;
    width: 0px;
}


